  "contact": {
    "form": {
      "fields": [
        {
          "name": "name",
          "type": "text",
          "required": true,
          "placeholder": "Your full name"
        },
        {
          "name": "email",
          "type": "email",
          "required": true,
          "placeholder": "your.email@example.com"
        },
        {
          "name": "phone",
          "type": "tel",
          "required": true,
          "placeholder": "Your phone number"
        },
        {
          "name": "message",
          "type": "textarea",
          "required": true,
          "placeholder": "Tell me about your vision for the shoot..."
        }
      ],
      "validation": {
        "name": {
          "minLength": 2,
          "maxLength": 50
        },
        "email": {
          "pattern": "^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$"
        },
        "phone": {
          "pattern": "^\\d{3}-\\d{3}-\\d{4}$"
        },
        "message": {
          "minLength": 10,
          "maxLength": 500
        }
      }
    },
    "whatsapp": {
      "enabled": true,
      "number": "+918888234987",
      "messageTemplate": "Hello, I'm interested in photography services. My details are: Name: {name}, Email: {email}, Phone: {phone}, Message: {message}"
    }
  },
